<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.web.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<fx:root type="javafx.scene.layout.VBox" xmlns:fx="http://javafx.com/fxml">
<VBox prefHeight="620.0" prefWidth="383.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane prefHeight="125.0" prefWidth="383.0" style="-fx-background-color: #66ccff;">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="uClear" textFill="WHITE" id="uclear">
               <font>
                  <Font size="34.0" />
               </font>
            </Label>
            <Label fx:id="popMessage" prefWidth="280.0" text="Good! All tasks are clear!" textFill="WHITE" GridPane.columnSpan="2" GridPane.rowIndex="1" id="assert">
               <font>
                  <Font size="17.0" />
               </font>
                  </Label>
            <Label fx:id="date" alignment="TOP_RIGHT" prefHeight="15.0" prefWidth="212.0" text="Date" textFill="WHITE" GridPane.columnIndex="1">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
         </children>
      </GridPane>
      <ScrollPane prefHeight="498.0" prefWidth="383.0" hbarPolicy="AS_NEEDED" hmax="1.0" pannable="false" vbarPolicy="AS_NEEDED">
         <content>
            <VBox fx:id="toBeCompleted" prefHeight="490.0" prefWidth="383.0" />
         </content></ScrollPane>
      <VBox alignment="BOTTOM_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"  prefHeight="63.0" style="-fx-padding: 10; -fx-background-color: #55bbee;">
         <children>
            <TextField fx:id="textField" prefHeight="42.0" prefWidth="383.0" id="textfield" onAction="#onEnter">
            </TextField>
         </children>
      </VBox>
   </children>
</VBox>
</fx:root>
